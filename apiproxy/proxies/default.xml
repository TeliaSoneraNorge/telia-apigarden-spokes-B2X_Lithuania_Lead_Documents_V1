<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>Spike-Arrest</Name>
            </Step>
            <Step>
                <Name>Set-Flow-Vars</Name>
            </Step>
            <Step>
                <Name>Verify-oAuth-V2</Name>
            </Step>
            <Step>
                <Name>Get-Secrets</Name>
            </Step>
            <Step>
                <Name>Get-value-from-KVM</Name>
            </Step>
            <Step>
                <Name>Set-variables</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <Flows/>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <HTTPProxyConnection>
        <BasePath>/B2X_Lithuania_Lead_Documents_V1,</BasePath>
        <VirtualHost>https</VirtualHost>
        <VirtualHost>secure</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="default">
        <TargetEndpoint>default</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>
